{
  "schemaVersion": "0.11",
  "fields": [
    {
      "name": "tradeId",
      "type": "integer",
      "nullable": false,
      "unique": true
    },
    {
      "name": "stock",
      "type": "string",
      "nullable": false
    },
    {
      "name": "side",
      "type": "string",
      "nullable": false
    },
    {
      "name": "tradePrice",
      "type": "decimal",
      "nullable": false
    },
    {
      "name": "volume",
      "type": "integer",
      "nullable": false
    },
    {
      "name": "tradeDatetime",
      "type": "datetime",
      "nullable": false
    }
  ],
  "rules": [
    {
      "rule": "tradeId",
      "constraints": [
        {
          "greaterThanOrEqualTo": {
            "field": "tradeId",
            "value": 10000
          }
        },
        {
          "lessThanOrEqualTo": {
            "field": "tradeId",
            "value": 99999
          }
        }
      ]
    },
    {
      "rule": "stock",
      "constraints": [
        {
          "inSet": {
            "field": "stock",
            "file": "examples/demo-trades/stockCodes.csv"
          }
        }
      ]
    },
    {
      "rule": "side",
      "constraints": [
        {
          "inSet": {
            "field": "side",
            "values": [
              "BUY",
              "SELL"
            ]
          }
        }
      ]
    },
    {
      "rule": "tradePrice",
      "constraints": [
        {
          "granularTo": {
            "field": "tradePrice",
            "value": 0.01
          }
        },
        {
          "greaterThan": {
            "field": "tradePrice",
            "value": 140.0
          }
        },
        {
          "lessThan": {
            "field": "tradePrice",
            "value": 150.0
          }
        }
      ]
    },
    {
      "rule": "volume",
      "constraints": [
        {
          "greaterThanOrEqualTo": {
            "field": "volume",
            "value": 1
          }
        },
        {
          "lessThanOrEqualTo": {
            "field": "volume",
            "value": 200
          }
        }
      ]
    },
    {
      "rule": "tradeDatetime",
      "constraints": [
        {
          "after": {
            "field": "tradeDatetime",
            "value": "2019-09-02T09:30:00.000"
          }
        },
        {
          "before": {
            "field": "tradeDatetime",
            "value": "2019-09-02T16:30:00.000"
          }
        }
      ]
    }
  ]
}