{
  "schemaVersion": "0.10",
  "fields": [
    {
      "name": "first_name",
      "type": "string",
      "nullable": false
    }
  ],
  "rules": [
    {
      "constraints": [
        {
          "matchingRegex": {
            "field": "first_name",
            "value": "(Joh?n|Mar[yk])"
          }
        },
        {
          "matchingRegex": {
            "field": "first_name",
            "value": "M.*"
          }
        },
        {
          "not": {
          "equalTo": {
              "field": "first_name",
              "value": "Mary"
            }
          }
        }
      ]
    }
  ]
}