{
  "schemaVersion": "0.11",
  "rules": [
    {
      "rule": "rule 1",
      "constraints": [
        {
          "anyOf": [
            {
              "equalTo":
              {
              "field": "field1",
              "value": "foo"}
            },
            {
              "equalTo":
              {
                "field": "field1",
                "value": "bar"
              }
            }
          ]
        }
      ]
    }
  ],
  "fields": [
    {
      "name": "field1",
      "type": "string",
      "nullable": false
    }
  ]
}